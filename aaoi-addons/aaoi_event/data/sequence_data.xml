<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="0">

    <!-- Sequences -->
    <record id="seq_event_certificate" model="ir.sequence">
      <field name="name">Event Certificate</field>
      <field name="code">event.certificate</field>
      <field name="prefix">%(year)s-AAOI_</field>
      <field name="padding">3</field>
      <field eval="1" name="number_increment"/>
    </record>

    <record id="seq_event_testimonial" model="ir.sequence">
      <field name="name">Event Testimonial</field>
      <field name="code">event.testimonial</field>
      <field name="prefix">TST/%(year)s-AAOI_</field>
      <field name="padding">3</field>
      <field eval="1" name="number_increment"/>
    </record>

  </data>
</odoo>
